{
    "version": 2,
    "builds": [
      {
        "src": "nuxt.config.ts",
        "use": "@vercel/node",
        "config": {
          "serverFiles": [
            ".nuxt/dist/server/**/*",
            ".output/**/*"
          ]
        }
      }
    ],
    "buildCommand": "NITRO_PRESET=vercel nuxt build",
    "outputDirectory": ".output",
    "routes": [
      {
        "src": "/_nuxt/(.*)",
        "headers": {
          "Cache-Control": "public,max-age=31536000,immutable"
        },
        "continue": true
      },
      {
        "src": "/(.*)",
        "headers": {
          "Cache-Control": "s-maxage=1, stale-while-revalidate"
        },
        "dest": ".output/server/index.mjs"
      }
    ]
  }